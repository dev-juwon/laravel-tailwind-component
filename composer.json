{
    "name": "laltu/laravel-tailwind-component",
    "description": "laravel-tailwind-component",
    "keywords": [
        "laltu",
        "laravel-tailwind-component"
    ],
    "homepage": "https://github.com/laltu/laravel-tailwind-component",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "laltu das",
            "email": "laltu.lspl@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Laltu\\LaravelTailwindComponent\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laltu\\LaravelTailwindComponent\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laltu\\LaravelTailwindComponent\\LaravelTailwindComponentServiceProvider"
            ],
            "aliases": {
                "LaravelTailwindComponent": "Laltu\\LaravelTailwindComponent\\LaravelTailwindComponentFacade"
            }
        }
    }
}
